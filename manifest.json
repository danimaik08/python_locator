{
  "manifest_version": 3,
  "name": "python_locator",
  "description": "Extraction of locators for Python autotests",
  "version": "1.4",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./dist/python_locator_main.js",
        "./dist/871.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "128": "./128.png"
  },
  "permissions": [
    "accessibilityFeatures.modify",
    "accessibilityFeatures.read",
    "activeTab",
    "clipboardWrite"
  ]
}